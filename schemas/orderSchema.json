{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:3000/shipments/order.schema.json",
    "title": "Order",
    "description": "Order for shipping company.",
    "type": "object",
    "properties": {
        "productId" : {
            "type" : "integer",
            "minimum": 1000
        },
        "name": {"type": "string"},
        "addr": {"type": "string"},
        "zip": {"type": "string"}
    },
    "additionalProperties": false,
    "required": [
        "productId",
        "name",
        "addr",
        "zip"
    ]
}


